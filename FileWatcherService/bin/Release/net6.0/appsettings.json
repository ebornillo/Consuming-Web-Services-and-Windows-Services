{
  "FolderSettings": {
    "SourceFolder": "C:\\Folder1",
    "DestinationFolder": "C:\\Folder2"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\FileWatcher-.log",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "EventLog",
        "Args": {
          "source": "FileWatcherService"
        }
      }
    ]
  }
}
